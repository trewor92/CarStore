@model DeclarationListViewModel
@foreach (Car c in Model.Cars)
{
    @Html.Partial("DeclarationSummary", c)
}
<div page-model="@Model.PagingInfo"
    page-action="List"
    page-classes-enabled="true"
    page-class="btn"
    page-class-normal="btn-default"
    page-class-selected="btn-primary"
    page-url-category=@Model.CurrentCategory
    class="btn-group pull-right"></div>